{
  "name": "conflu-exporter",
  "version": "0.1.0",
  "description": "A TypeScript library for exporting Confluence content",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "conflu": "./bin/conflu.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "confluence",
    "export",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.21.0",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4.3.0",
    "fs-extra": "^11.2.0",
    "html-docx-js-typescript": "^0.1.0",
    "jsdom": "^23.0.0",
    "p-limit": "^5.0.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "puppeteer": "^21.6.0",
    "turndown": "^7.1.0",
    "turndown-plugin-gfm": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^20.11.0",
    "@types/turndown": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "nock": "^13.5.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
